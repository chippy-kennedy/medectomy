<% @enrollments.each do |enrollment| %>
	<%= button_to enrollment, :method => :delete, id: "enroll_course_#{enrollment.course_id}", remote: true do %>
		<i style="font-size:20px;" class="fa fa-minus">
		</i>
	<% end %>
<% end %>
<% @not_enrolled_in.each do |course| %>
	<%= button_to enrollments_path(course_id: course.id), :method => :post, id: "enroll_course_#{course.id}", remote: true do %>
		<i style="font-size:20px;" class="fa fa-plus">
		</i>
	<% end %>
<% end %>